@article{Papadodimas2007,
abstract = {We study the thermodynamics of large N pure 2+1 dimensional Yang-Mills theory on a small spatial S 2
 . By studying the effective action for the Polyakov loop order parameter, we show analytically that
 the theory has a second order deconfinement transition to a phase where the eigenvalue distribution
