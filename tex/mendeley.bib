issn = {1029-8479},
journal = {Journal of High Energy Physics},
keywords = {Heavy Quarks Physics,Lattice Gauge Field Theories,Thermal Field Theory},
language = {en},
month = sep,
number = {09},
pages = {066--066},
publisher = {IOP Publishing},
title = {{Thermal imaginary part of a real-time static potential from classical lattice gauge theory simulations}},
url = {http://iopscience.iop.org/1126-6708/2007/09/066},
volume = {2007},
year = {2007}
}
@article{Liu2007,
abstract = {Expectation values of Wilson loops define the nonperturbative properties of the hot medium produced in heavy ion collisions that arise in the analysis of both radiative parton energy loss and quarkonium suppression. We use the AdS/CFT correspondence to calculate the expectation values of such Wilson loops in the strongly coupled plasma of = 4 super Yang-Mills (SYM) theory, allowing for the possibility that the plasma may be moving with some collective flow velocity as is the case in heavy ion collisions. We obtain the = 4 SYM values of the jet quenching parameter , which describes the energy loss of a hard parton in QCD, and of the velocity-dependence of the quark-antiquark screening length for a moving dipole as a function of the angle between its velocity and its orientation. We show that if the quark-gluon plasma is flowing with velocity vf at an angle $\theta$ with respect to the trajectory of a hard parton, the jet quenching parameter is modified by a factor $\gamma$f(1−vf cos $\theta$), and show that this result applies in QCD as in = 4 SYM. We discuss the relevance of the lessons we are learning from all these calculations to heavy ion collisions at RHIC and at the LHC. Furthermore, we discuss the relation between our results and those obtained in other theories with gravity duals, showing in particular that the ratio between in any two conformal theories with gravity duals is the square root of the ratio of their central charges. This leads us to conjecture that in nonconformal theories defines a quantity that always decreases along renormalization group trajectories and allows us to use our calculation of in = 4 SYM to make a conjecture for its value in QCD.},
author = {Liu, Hong and Rajagopal, Krishna and Wiedemann, Urs Achim},
doi = {10.1088/1126-6708/2007/03/066},
file = {:home/jahn/Documents/Mendeley//Liu, Rajagopal, Wiedemann. 2007. Wilson loops in heavy ion collisions and their calculation in AdSCFT.pdf:pdf},
issn = {1029-8479},
journal = {Journal of High Energy Physics},
keywords = {AdS-CFT and dS-CFT Correspondence,QCD},
language = {en},
month = mar,
number = {03},
pages = {066--066},
publisher = {IOP Publishing},
title = {{Wilson loops in heavy ion collisions and their calculation in AdS/CFT}},
url = {http://iopscience.iop.org/1126-6708/2007/03/066},
volume = {2007},
year = {2007}
}
