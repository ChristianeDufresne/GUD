keywords = {1/N Expansion,3-LOOP BETA-FUNCTION,FINITE-TEMPERATURE,FREE-ENERGY,Field Theories in Lower Dimensions,Holography and quark-gluon plasmas,LARGE-N-C,LATTICE GAUGE-THEORY,Lattice Gauge Field Theories,MONTE-CARLO CALCULATIONS,QCD,QUARK-GLUON PLASMA,STRING THEORY,WILSON FERMIONS},
month = may,
number = {5},
